pipeline {
    agent any
    tools{
        maven 'Maven'
    }
    
    stages {
        stage('Clean')
        {
             steps {
                    bat 'mvn -f passport/pom.xml clean'
                
            }
        }
        stage('Compile')
        {
             steps {
                    bat 'mvn -f passport/pom.xml compile'
                
            }
        }
       
        stage('Package') {
            steps {
                    bat 'mvn -f passport/pom.xml package'
                
            }
        }
        stage('Test') { 
            steps {
                bat 'mvn -f passport/pom.xml test' 
            }
            
        }
    }
}
